{
  "name": "sveltui",
  "version": "0.1.0",
  "description": "Write Svelte applications for terminal with SvelTUI",
  "main": "dist/src/index.cjs",
  "module": "dist/src/index.mjs",
  "type": "module",
  "author": "RLabs Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rlabs/sveltui.git"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "terminal",
    "tui",
    "renderer",
    "cli",
    "blessed",
    "ink"
  ],
  "scripts": {
    "build": "bun scripts/build.mjs",
    "build:watch": "bun scripts/build.mjs --watch",
    "build:verbose": "bun scripts/build.mjs --verbose",
    "demo": "bun run build && bun --conditions browser dist/src/main.mjs",
    "demo:counter": "bun run build && bun --conditions browser dist/examples/counter/main.mjs",
    "package": "bun run build && bun scripts/package.mjs",
    "create-app": "bun scripts/create-app.mjs",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@types/blessed": "0.1.25",
    "@types/bun": "latest",
    "@types/js-yaml": "4.0.9",
    "@types/node": "24.1.0",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.8",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3",
    "vite": "7.0.6",
    "vite-node": "3.1.3"
  },
  "dependencies": {
    "blessed": "0.1.81",
    "happy-dom": "18.0.1",
    "js-yaml": "4.1.0",
    "svelte": "^5.30.2",
    "yoga-layout-prebuilt": "^1.10.0"
  },
  "browser": {
    "svelte": "svelte/src/index-client.js"
  },
  "imports": {
    "svelte": "svelte/src/index-client.js"
  }
}
