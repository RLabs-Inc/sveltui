{
  "name": "sveltui",
  "module": "index.ts",
  "type": "module",
  "version": "0.1.0",
  "author": "RLabs Inc.",
  "license": "MIT",
  "description": "A true Svelte 5 renderer for terminal user interfaces",
  "repository": {
    "type": "git",
    "url": "https://github.com/rlabs/sveltui.git"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "terminal",
    "tui",
    "renderer",
    "cli",
    "blessed",
    "ink"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "example": "bun --conditions browser examples/index.ts",
    "compile": "node scripts/compile-svelte.mjs",
    "compile:counter": "node scripts/compile-svelte.mjs examples/simple-counter/App.svelte",
    "test": "bun test",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "debug": "NODE_ENV=development DEBUG=true bun --inspect examples/index.ts"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@types/blessed": "0.1.25",
    "@types/bun": "latest",
    "@types/js-yaml": "4.0.9",
    "@types/node": "24.1.0",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.8",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3",
    "vite": "7.0.6",
    "vite-node": "3.1.3"
  },
  "dependencies": {
    "blessed": "0.1.81",
    "happy-dom": "18.0.1",
    "js-yaml": "4.1.0",
    "svelte": "^5.30.2",
    "yoga-layout-prebuilt": "^1.10.0"
  },
  "browser": {
    "svelte": "svelte/src/index-client.js"
  },
  "imports": {
    "svelte": "svelte/src/index-client.js"
  }
}
